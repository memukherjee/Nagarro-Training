  <script > 
    var time=prompt("Can you tell me the time ");
    if(time >5 && time< 12) 
      {
        alert("GOOD MORNING");
      }
     if(time >12 && time< 17) 
      {
        alert("GOOD AFTERNOON");
      }
     if(time >17 && time< 21) 
      {
        alert("GOOD EVENING");
      }
    else 
      {
        alert("Good Night!")
      }
  </script>
